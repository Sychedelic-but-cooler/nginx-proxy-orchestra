{
  "name": "nginx-proxy-orchestra",
  "version": "1.0.0",
  "description": "A minimal web UI for managing nginx reverse proxy configurations",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "NODE_ENV=development node server/index.js",
    "test": "node tests/input-validator.test.js",
    "test:api": "node tests/api-injection.test.js",
    "test:all": "npm test && npm run test:api",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:start:watch": "pm2 start ecosystem.config.js --watch",
    "pm2:stop": "pm2 stop nginx-proxy-orchestra",
    "pm2:restart": "pm2 restart nginx-proxy-orchestra",
    "pm2:reload": "pm2 reload nginx-proxy-orchestra",
    "pm2:delete": "pm2 delete nginx-proxy-orchestra",
    "pm2:logs": "pm2 logs nginx-proxy-orchestra --lines 100",
    "pm2:status": "pm2 status nginx-proxy-orchestra",
    "pm2:monit": "pm2 monit"
  },
  "keywords": [
    "nginx",
    "reverse-proxy",
    "webui",
    "admin"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "better-sqlite3": "^11.0.0",
    "dotenv": "^16.0.0",
    "ipaddr.js": "^2.3.0",
    "jsonwebtoken": "^9.0.0",
    "node-cron": "^3.0.3",
    "otplib": "^12.0.1",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "pm2": "^6.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
